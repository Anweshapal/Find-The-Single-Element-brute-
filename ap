public class Solution {
    public static int getSingleElement(int []arr){
        for(int i=0;i<arr.length;i++){
            int num=arr[i];
            int count=0;
            for(int j=0;j<arr.length;j++){
                if(arr[j]==num){
                    count++;
                }
            }
            if(count==1){
                return num;
            }
        }
        return -1;
    }
}
